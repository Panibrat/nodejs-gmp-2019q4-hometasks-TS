{
  "name": "nodejs-hometasks-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/index.js",
    "start-server": "nodemon --exec dist/index.js",
    "db:create": "cd src && npx sequelize-cli db:create",
    "db:migrate": "cd src && npx sequelize-cli db:migrate",
    "db:seed": "cd src && npx sequelize-cli db:seed:all"
  },
  "author": "Oleksandr Panibratenko",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/body-parser": "^1.17.1",
    "@types/node": "^12.7.8",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "nodemon": "^2.0.1",
    "eslint": "^6.4.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "standard-http-error": "^2.0.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  }
}
